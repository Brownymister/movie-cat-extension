{
  "manifest_version": 3,
  "name": "MovieCat",
  "version": "0.0.1",
  "description": "MovieCat",
  "icons": {
    "16": "icons/icon-16x16.png",
    "128": "icons/icon-128x128.png",
    "256": "icons/icon-256x256.png"
  },
  "action": {
    "default_title": "Click to open MovieCat."
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
